.knowledge_container
    display: table
    width: 343px
    height 161px
    background-color #fff
    border-radius: 30px 0
    padding 15px
    margin:0 auto 40px
.knowledge_img
    display: table-cell
    vertical-align: middle
    width: 50px
    background-image url(../img/cause_icons.jpg)
    background-repeat no-repeat
    background-position:  0 center
.knowledge_img_second
    background-position -50px center
.knowledge_img_third
    background-position -100px center
.knowledge_img_fourth
    background-position -150px center
.knowledge_img_fifth
    background-position -200px center
.knowledge_img_sixth
    background-position -250px center
.knowledge
    display: table-cell
    vertical-align: middle
    font-size: 16px;
    font-family: "RobotoRegular",sans-serif;
    color: rgb(60, 68, 87);
    line-height: 1.5;
    padding-left: 20px